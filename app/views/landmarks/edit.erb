<h1>Edit Landmark</h1>
<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label for="landmark_name">Edit Landmark</label>
  <br></br>

  <input type="text" id="landmark_name" name="landmark[name]" value="<%=@landmark.name%>">

  <br></br>

     <label for ="year">Year:</label>
     <input type="text" id="year" name="landmark[year_completed]" value="<%=@landmark.year_completed%>">
    <input type="submit" value="Edit Landmark">
  </form>